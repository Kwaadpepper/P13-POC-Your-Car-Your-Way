{
  "conventionalCommits.scopes": [
    "back",
    "back-auth",
    "back-support",
    "back-user"
  ],
  "editor.formatOnSave": true,
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.import.gradle.enabled": true,
  "java.import.gradle.wrapper.enabled": true,
  "java.import.gradle.offline.enabled": false,
  "java.import.generatesMetadataFilesAtProjectRoot": true,
  "java.compile.nullAnalysis.mode": "automatic",
  "java.format.comments.enabled": true,
  "java.format.enabled": false,
  "[java]": {
    "editor.formatOnSave": false,
    "files.trimTrailingWhitespace": false,
  },
  "triggerTaskOnSave.tasks": {
    "Gradle Spotless Apply Silently": [
      "*.java",
      "*.kts",
    ],
  },
  "java.checkstyle.configuration": "config/checkstyle/checkstyle.xml",
  "java.checkstyle.properties": {
    "org.checkstyle.google.suppressionfilter.config": "${workspaceFolder}/config/checkstyle/checkstyle-suppressions.xml"
  },
  "java.checkstyle.version": "11.0.0",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#c4350c",
    "activityBar.background": "#c4350c",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#043c10",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#c4350c",
    "statusBar.background": "#942809",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#c4350c",
    "statusBarItem.remoteBackground": "#942809",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#942809",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#94280999",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#942809",
  "java.project.explorer.showNonJavaResources": false,
  "java.dependency.packagePresentation": "hierarchical",
  "java.dependency.syncWithFolderExplorer": true
}
