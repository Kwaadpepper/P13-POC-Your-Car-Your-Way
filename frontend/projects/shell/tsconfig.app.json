{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "types": [],
    "paths": {
      "~shell-core/*": [
        "./src/app/core/*"
      ],
      "~shell-env/*": [
        "./src/environments/*"
      ],
      "~shell-domains/*": [
        "./src/app/domains/*"
      ],
      "~shell-shared/*": [
        "./src/app/shared/*"
      ],
      "~ycyw/shared": [
        "../../dist/shared"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ]
}
