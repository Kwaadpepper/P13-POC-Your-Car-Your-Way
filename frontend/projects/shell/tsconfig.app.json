{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "types": [],
    "paths": {
      "@ycyw/shell-core/*": [
        "./src/app/core/*"
      ],
      "@ycyw/shell-env/*": [
        "./src/environments/*"
      ],
      "@ycyw/shell-domains/*": [
        "./src/app/domains/*"
      ],
      "@ycyw/shell-shared/*": [
        "./src/app/shared/*"
      ],
      "@ycyw/shell-tokens/*": [
        "./src/app/tokens/*"
      ],
      "@ycyw/shared": [
        "../../dist/shared"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "../shared/src/lib/errors/bad-response-error.ts",
    "../shared/src/lib/errors/session-expired-error.ts",
    "../shared/src/lib/utils/check-server-response.ts",
    "../shared/src/lib/utils/verify-response-type.ts",
    "../shared/src/schemas/validation-error-schema.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ]
}
